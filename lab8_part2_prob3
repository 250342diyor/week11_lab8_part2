#include <iostream>
#include <string>
using namespace std;
bool isAnagram(string s, string t) {
    if (s.length() != t.length()) {
        return false;
    }
    int charCounts[26] = {0};
    for (int i = 0; i < s.length(); i++) {
        charCounts[s[i] - 'a']++;
        charCounts[t[i] - 'a']--;
    }
    for (int i = 0; i < 26; i++) {
        if (charCounts[i] != 0) {
            return false;
        }
    }
    return true;
}
int main() {
    string s;
    string t;
    cout << "Enter string: ";
    cin >> s;
    cout << "Enter string: ";
    cin >> t;
    if (isAnagram(s, t)) {
        cout << "true"<<endl;
    }else {
        cout << "false"<<endl;
    }
}
